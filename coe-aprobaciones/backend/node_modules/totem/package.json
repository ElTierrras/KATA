{
    "name": "totem",
    "version": "5.2.0",
    "author": "Robert Dallas Gray <robert.dallas.gray@huzutech.com>",
    "description": "A library to easily manipulate avatar data in HAPI.",
    "keywords": [
        "hapi",
        "avatars"
    ],
    "dependencies": {
        "hapilib": "0.6.x"
    },
    "devDependencies": {
        "karma": "0.8.x",
        "browserify": "2.12.x",
        "coffee-script": "1.6.x",
        "codo": "1.6.x",
        "hapi.app.tools": "0.0.x"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/HuzuTech/Totem.git"
    },
    "scripts": {
